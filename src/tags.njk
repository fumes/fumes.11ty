---
templateEngineOverride: njk
eleventyExcludeFromCollections: true
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
   - tagList
  addAllPagesToCollections: true
layout: default
renderData:
  title: Tagged “{{ tag }}”
permalink: /{{ tag }}/
---
<h1>Tagged “{{ tag }} ”</h1>

{% set postslist = collections[ tag ] %}


<div class="home__post">

  {% for post in postslist | reverse %}

    {% include "partials/index_post_block--excerpt.njk" %}

  {% endfor %}

</div>



<p>See <a href="{{ '/tags/' | url }}">all tags</a>.</p>
